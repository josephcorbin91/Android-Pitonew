<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"


    android:id="@+id/resultFragmnet"
    tools:context=".Fragments.ResultFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!-- TODO: Update blank fragment layout -->
        <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >



            <TableRow
                android:layout_margin="@dimen/InputEditTextMargin"
                >


                <TextView
                    android:textColor="@color/text_color"
                    android:text="Average Velocity"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:id="@+id/averageVelocityResultTextView"
                                android:textColor="@color/text_color"

                                android:layout_width="@dimen/InputTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:text="m/s"
                                android:textColor="@color/text_color"
                                android:id="@+id/UnitsaverageVelocityTextView"

                                android:textSize="@dimen/input_fragment_text_size"/>
            </TableRow>
            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >


                <TextView
                    android:textColor="@color/text_color"
                    android:text="Mass Air Flow"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>

                            <TextView
                                android:id="@+id/massAirFlowResultTextView"
                                android:textColor="@color/text_color"

                                android:layout_width="@dimen/InputTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:textColor="@color/text_color"
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:text="C"
                                android:id="@+id/UnitsmassAirFlowTextView"

                                android:textSize="@dimen/input_fragment_text_size"/>


</TableRow>


            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >

                <TextView
                    android:textColor="@color/text_color"
                    android:text="Actual Air Flow"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="@dimen/InputTextViewTextWidth"
                                android:textColor="@color/text_color"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:id="@+id/actualAirFlowResultTextView"

                                android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:textColor="@color/text_color"
                                android:text="C"
                                android:id="@+id/UnitsactualAirFlowTextView"
                                android:textSize="@dimen/input_fragment_text_size"/>

            </TableRow>

            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >

                <TextView
                    android:textColor="@color/text_color"
                    android:text="Normal Air Flow"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:textColor="@color/text_color"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:id="@+id/normalAirFlowResultTextView"

                    android:textSize="@dimen/input_fragment_text_size"/>
                <TextView
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textColor="@color/text_color"
                    android:text="C"
                    android:id="@+id/UnitsNormalAirFlowTextView"
                    android:textSize="@dimen/input_fragment_text_size"/>

            </TableRow>

            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >


                <TextView
                    android:textColor="@color/text_color"
                    android:text="Molar Weight"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>

                <TextView
                    android:layout_width="100sp"
                    android:layout_height="wrap_content"
                    android:id="@+id/molecularWeightResultTextView"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/input_fragment_text_size"/>

                <TextView
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:textColor="@color/text_color"
                    android:layout_height="@dimen/InputEditTextHeight"

                    android:text="kg/mol"
                    android:id="@+id/UnitsMolarWeight"/>




            </TableRow>

            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >


                <TextView
                    android:textColor="@color/text_color"
                    android:text="Duct Presure"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="@dimen/InputTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:textColor="@color/text_color"
                                android:id="@+id/ductPressureFragmentTextView"

                                android:textSize="@dimen/input_fragment_text_size"/>

                            <TextView
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:textColor="@color/text_color"
                                android:layout_height="@dimen/InputEditTextHeight"

                                android:text="°KPa"
                                android:id="@+id/UnitsDuctPressureFragmentTextView"/>

            </TableRow>
            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >

                <TextView
                    android:textColor="@color/text_color"
                    android:text="Area"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>

                            <TextView
                                android:layout_width="100sp"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:textColor="@color/text_color"
                                android:id="@+id/ductAreaGasFlowFragmentTextView"
                                android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:textColor="@color/text_color"
                                android:id="@+id/ductAreaGasFlowFragmentTextViewUnits"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:text="m²"
                                android:textSize="@dimen/input_fragment_text_size"/>
                </TableRow>


            <TableRow
                >

                <TextView
                    android:textColor="@color/text_color"
                    android:text="Relative Humidity"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="100sp"
                                android:textColor="@color/text_color"
                                android:layout_height="wrap_content"

                                android:id="@+id/relativeHumidityResultTextView"
                                android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:textColor="@color/text_color"
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:text="%"
                                android:id="@+id/UnitsRelativeHumiditytemperatureGasDensityFragmentTextView"/>
                </TableRow>
            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >

                <TextView
                    android:textColor="@color/text_color"
                    android:text="Atmospheric Pressure"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:id="@+id/gasAtmosphericPressureResultTextView"
                                android:textColor="@color/text_color"

                                android:textSize="@dimen/input_fragment_text_size"/>
                            <TextView
                                android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                                android:layout_height="@dimen/InputEditTextHeight"
                                android:text="ft"
                                android:id="@+id/UnitsAtmosphericPressureFragmentTextViewUnits"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/input_fragment_text_size"/>
            </TableRow>












            <TableRow                 android:layout_margin="@dimen/InputEditTextMargin"                 >


                <TextView
                    android:textColor="@color/text_color"
                    android:text="Gas Density"

                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/InputEditTextHeight"
                    android:textSize="@dimen/input_fragment_text_size"/>
            <TextView
                            android:layout_width="100sp"
                            android:layout_height="wrap_content"
                            android:id="@+id/gasDesnityResultTextView"
                            android:textColor="@color/text_color"
                            android:textSize="@dimen/input_fragment_text_size"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="kg/m³"
                            android:textColor="@color/text_color"
                            android:id="@+id/UnitsCalculatedGasDensityTextView"
                            android:textSize="@dimen/input_fragment_text_size"/>


            </TableRow>

        </TableLayout>

    </ScrollView>
</FrameLayout>
