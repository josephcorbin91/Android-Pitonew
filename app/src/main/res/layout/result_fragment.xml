<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"


    android:id="@+id/resultFragmnet"
    tools:context=".Fragments.ResultFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <!-- TODO: Update blank fragment layout -->
        <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:gravity="center_horizontal"
                android:text="Gas Flow Calculations"
                android:textSize="24dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="4dp"
                android:gravity="center_horizontal"
                android:text="Press Return to modify input values"
                android:textSize="16dp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="16dp"
                android:background="@color/PrimaryColor" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:background="@color/SecondaryColor">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center|left"
                    android:paddingLeft="16dp"
                    android:text="Results"
                    android:textColor="#ffffff"
                    android:textSize="16dp" />


            </TableRow>


            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">


                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_weight="0.6"

                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/tile_height_full_width_single_line"
                    android:gravity="left|center"
                    android:text="Dynamic Velocity"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/dynamicVelocityResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.3"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:minHeight="@dimen/tile_height_full_width_single_line"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="0.1"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />
            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">


                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="@dimen/InputTextViewTextWidth"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="left|center"
                    android:text="Average Velocity"
                    android:layout_weight="0.6"


                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/averageVelocityResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.3"

                    android:gravity="center"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsaverageVelocityTextView"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:text="m/s"
                    android:layout_weight="0.1"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />
            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">


                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_weight="0.6"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="left|center"
                    android:text="Mass Air Flow"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/massAirFlowResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsmassAirFlowTextView"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:gravity="center"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:text="kg/s"
                    android:layout_weight="0.1"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />


            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.6"

                    android:gravity="left|center"
                    android:text="Actual Air Flow"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/actualAirFlowResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:textColor="@color/text_color"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsactualAirFlowTextView"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:gravity="center"
                    android:layout_weight="0.1"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:text="m³/h"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.6"

                    android:gravity="left|center"
                    android:text="Normal Air Flow"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/normalAirFlowResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textColor="@color/text_color"

                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsNormalAirFlowTextView"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:text="Nm³/h"
                    android:gravity="center"
                    android:layout_weight="0.1"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">


                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.6"

                    android:gravity="left|center"
                    android:text="Molar Weight"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/molecularWeightResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsMolarWeight"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:layout_weight="0.1"

                    android:text="g/mol"
                    android:textColor="@color/text_color" />


            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">


                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"

                    android:layout_weight="0.6"
                    android:gravity="left|center"
                    android:text="Duct Presure"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/ductPressureFragmentTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:textColor="@color/text_color"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsDuctPressureFragmentTextView"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:layout_weight="0.1"

                    android:text="°KPa"
                    android:textColor="@color/text_color" />

            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.6"

                    android:gravity="left|center"
                    android:text="Area"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/ductAreaGasFlowFragmentTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:textColor="@color/text_color"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/ductAreaGasFlowFragmentTextViewUnits"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.1"
                    android:gravity="center"
                    android:text="m²"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />
            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.6"

                    android:gravity="left|center"
                    android:text="Relative Humidity"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/relativeHumidityResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsRelativeHumiditytemperatureGasDensityFragmentTextView"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_weight="0.1"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:text="%"
                    android:gravity="center"

                    android:textColor="@color/text_color" />
            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"
                    android:layout_weight="0.6"

                    android:text="Atmospheric Pressure"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/gasAtmosphericPressureResultTextView"
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:textColor="@color/text_color"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsAtmosphericPressureFragmentTextViewUnits"
                    android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:text="kPa"
                    android:gravity="center"
                    android:layout_weight="0.1"

                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />
            </TableRow>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />


            <TableRow
                android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
                android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
                android:layout_marginRight="@dimen/single_line_list_left_right_padding"
                android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">


                <TextView
                    android:layout_width="@dimen/InputTextViewTextWidth"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:layout_weight="0.6"
                    android:gravity="left|center"

                    android:text="Gas Density"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/gasDesnityResultTextView"
                    android:layout_width="100sp"
                    android:gravity="center"
                    android:layout_weight="0.3"

                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />

                <TextView
                    android:id="@+id/UnitsCalculatedGasDensityTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/tile_height_full_width_single_line"
                    android:gravity="center"

                    android:layout_weight="0.1"
                    android:text="kg/m³"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/hint_and_input_text" />


            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />
        </TableLayout>

    </ScrollView>
</FrameLayout>
