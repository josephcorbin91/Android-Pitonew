<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:elevation="2dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <!-- TODO: Update blank fragment layout -->
    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TableRow
            android:id="@+id/dimension_1_table_row"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
            android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
            android:layout_marginRight="@dimen/single_line_list_left_right_padding"

            android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

            <TextView

                android:id="@+id/dimension1TextView"
                android:layout_width="@dimen/InputTextViewTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@android:color/transparent"

                android:gravity="left|center"
                android:maxLines="1"
                android:text="C02 %"
                android:textSize="@dimen/hint_and_input_text" />

            <EditText
                android:id="@+id/C02EditText"
                android:layout_width="@dimen/InputEditTextTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@drawable/highlighted_shape"
                android:gravity="center"
                android:imeOptions="actionNext"

                android:inputType="numberDecimal"
                android:maxLength="6"
                android:maxLines="1"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="@color/text_color"
                android:textSize="@dimen/hint_and_input_text" />


        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
            android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
            android:layout_marginRight="@dimen/single_line_list_left_right_padding"
            android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

            <TextView

                android:layout_width="@dimen/InputTextViewTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@android:color/transparent"

                android:gravity="left|center"
                android:maxLines="1"
                android:text="02 %"
                android:textSize="@dimen/hint_and_input_text" />

            <EditText
                android:id="@+id/O2EditText"

                android:layout_width="@dimen/InputEditTextTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@drawable/highlighted_shape"
                android:gravity="center"
                android:imeOptions="actionNext"

                android:inputType="numberDecimal"
                android:maxLength="6"
                android:maxLines="1"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="@color/text_color"
                android:textSize="@dimen/hint_and_input_text" />


        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
            android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
            android:layout_marginRight="@dimen/single_line_list_left_right_padding"
            android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

            <TextView

                android:layout_width="@dimen/InputTextViewTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@android:color/transparent"

                android:gravity="left|center"
                android:maxLines="1"
                android:text="N2 %"
                android:textSize="@dimen/hint_and_input_text" />

            <EditText
                android:id="@+id/N2EditText"
                android:layout_width="@dimen/InputEditTextTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@drawable/highlighted_shape"
                android:gravity="center"
                android:imeOptions="actionNext"

                android:inputType="numberSigned|numberDecimal"
                android:maxLength="6"
                android:maxLines="1"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="@color/text_color"
                android:textSize="@dimen/hint_and_input_text" />


        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"

            android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
            android:layout_marginRight="@dimen/single_line_list_left_right_padding"
            android:layout_marginTop="@dimen/single_line_list_top_bottom_padding">

            <TextView

                android:layout_width="@dimen/InputTextViewTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@android:color/transparent"
                android:gravity="left|center"

                android:maxLength="4"
                android:maxLines="1"
                android:text="Ar %"
                android:textSize="@dimen/hint_and_input_text" />

            <EditText
                android:id="@+id/ArEditText"
                android:layout_width="@dimen/InputEditTextTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@drawable/highlighted_shape"
                android:gravity="center"
                android:imeOptions="actionNext"

                android:inputType="numberDecimal"
                android:maxLength="6"
                android:maxLines="1"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="@color/text_color"
                android:textSize="@dimen/hint_and_input_text" />


        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/single_line_list_top_bottom_padding"
            android:layout_marginLeft="@dimen/single_line_list_left_right_padding"
            android:layout_marginRight="@dimen/single_line_list_left_right_padding"
            android:layout_marginTop="@dimen/single_line_list_top_bottom_padding"

            >

            <TextView

                android:layout_width="@dimen/InputTextViewTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@android:color/transparent"

                android:gravity="left|center"
                android:maxLines="1"
                android:text="H20 %"
                android:textSize="@dimen/hint_and_input_text" />

            <EditText
                android:id="@+id/H2OEditText"
                android:layout_width="@dimen/InputEditTextTextWidthDialog"
                android:layout_height="@dimen/TileHeight"
                android:background="@drawable/highlighted_shape"
                android:gravity="center"
                android:imeOptions="actionDone"
                android:inputType="numberSigned"
                android:maxLength="6"
                android:maxLines="1"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="@color/text_color"
                android:textSize="@dimen/hint_and_input_text" />


        </TableRow>
    </TableLayout>
</LinearLayout>