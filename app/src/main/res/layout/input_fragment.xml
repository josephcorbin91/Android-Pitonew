<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
   android:layout_height="fill_parent">



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <!-- TODO: Update blank fragment layout -->
    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">

            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_height="@dimen/TileHeight"
                android:text="Rectangular / Circular"
                android:layout_gravity="left"
                android:id="@+id/rectangularCircularGasFlowFragmentTextView"/>

            <Switch
                android:layout_width="@dimen/TileHeight"
                android:layout_height="wrap_content"
                android:id="@+id/rectangularCircularGasFlowFragmentSwitch"
                android:gravity="right"
                android:checked="false"
                />
        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Standard Air"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_gravity="left" />
            <Switch
                android:layout_width="100dp"
                android:layout_height="@dimen/TileHeight"

                android:id="@+id/standardAirSwitch"
                android:gravity="right"
                />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">

            <TextView

                android:layout_width="@dimen/InputTextViewTextWidth"
                android:textColor="@color/text_color"
                android:background="@android:color/transparent"
                android:maxLines="1"

                android:text="Duct Width"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_height="@dimen/TileHeight"
                />
                        <EditText
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:maxLines="1"
                            android:imeOptions="actionNext"
                            android:inputType="numberSigned"

                            android:textColor="@color/text_color"
                            android:layout_height="@dimen/TileHeight"
                            android:id="@+id/dimension_1_WidthGasEditText"
                            android:background="@drawable/shape"
                            android:hint="Duct Width"
                            android:textSize="@dimen/hint_and_input_text"/>
                        <TextView
                            android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                            android:textColor="@color/text_color"
                            android:layout_height="@dimen/TileHeight"

                            android:text="m"
                            android:id="@+id/UnitsDimension_1_WidthGasFragmentTextView"
                            android:textSize="@dimen/hint_and_input_text"/>

        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
         <TableRow
             android:layout_marginTop="@dimen/text_top_and_bottom_padding"
             android:layout_marginBottom="@dimen/text_top_and_bottom_padding">
             <TextView

                 android:layout_width="@dimen/InputTextViewTextWidth"
                 android:textColor="@color/text_color"
                 android:background="@android:color/transparent"
                 android:maxLines="1"

                 android:text="Duct Height"
                 android:textSize="@dimen/hint_and_input_text"
                 android:layout_height="@dimen/TileHeight"
                 />
             <EditText
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:textColor="@color/text_color"
                 android:background="@drawable/shape"
                            android:maxLines="1"

                 android:imeOptions="actionNext"
                 android:inputType="numberSigned"
                            android:hint="Duct Height"
                            android:id="@+id/dimension_2_HeightGasEditText"
                            android:layout_height="@dimen/TileHeight"
                            android:textSize="@dimen/hint_and_input_text"/>
                        <TextView
                            android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                            android:layout_height="@dimen/TileHeight"
                            android:textColor="@color/text_color"

                            android:text="m"
                            android:id="@+id/UnitsDimension_2_HeightGasFragmentTextView"
                            android:textSize="@dimen/hint_and_input_text"/>

        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">
        <TextView

                android:layout_width="@dimen/InputTextViewTextWidth"
                android:textColor="@color/text_color"
                android:background="@android:color/transparent"
                android:maxLines="1"

                android:text="Pitot Tube Coeffecient"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_height="@dimen/TileHeight"
                />
                        <EditText
                            android:textColor="@color/text_color"
                            android:maxLines="1"
                            android:imeOptions="actionNext"
                            android:inputType="numberSigned"
                            android:background="@drawable/shape"
                            android:hint="Pitot Tube Coeffecient"
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:id="@+id/pitotTubeCoefficientEditText"
                            android:layout_height="@dimen/TileHeight"
                            android:textSize="@dimen/hint_and_input_text"/>
        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
        android:layout_marginTop="@dimen/text_top_and_bottom_padding"
        android:layout_marginBottom="@dimen/text_top_and_bottom_padding">


    <TextView

        android:layout_width="@dimen/InputTextViewTextWidth"
        android:textColor="@color/text_color"
        android:background="@android:color/transparent" 
        android:maxLines="1"

        android:text="Static Pressure"
        android:textSize="@dimen/hint_and_input_text"
        android:layout_height="@dimen/TileHeight"
        />
                        <EditText
                            android:background="@drawable/shape"
                            android:maxLines="1"
                            android:imeOptions="actionNext"
                            android:inputType="numberSigned"

                            android:hint="Static Pressure"
                            android:textColor="@color/text_color"
                            android:id="@+id/staticPressureFragmentEditText"
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:layout_height="@dimen/TileHeight"
                            android:textSize="@dimen/hint_and_input_text"/>


    <TextView
        android:layout_width="@dimen/InputUnitsTextViewTextWidth"
        android:layout_height="@dimen/TileHeight"
        android:textColor="@color/text_color"
        android:text="kPa"
        android:id="@+id/UnitStaticPressureGasDensityFragmentTextView"

        android:textSize="@dimen/hint_and_input_text"/>


        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">
        <TextView

                android:layout_width="@dimen/InputTextViewTextWidth"
                android:textColor="@color/text_color"
                android:background="@android:color/transparent"
                android:maxLines="1"

                android:text="Dry Bulb Temperature"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_height="@dimen/TileHeight"
                />
               <EditText
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:layout_height="@dimen/TileHeight"
                            android:id="@+id/temperatureGasFragmentEditText"
                            android:textColor="@color/text_color"
                   android:background="@drawable/shape"
                            android:maxLines="1"

                   android:imeOptions="actionNext"
                   android:inputType="numberSigned"
                            android:hint="Dry Bulb Temperature"
                            android:textSize="@dimen/hint_and_input_text"/>

                        <TextView
                            android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                            android:layout_height="@dimen/TileHeight"

                            android:textColor="@color/text_color"
                            android:text="°C"
                            android:id="@+id/UnitstemperatureGasDensityFragmentTextView"
                            android:textSize="@dimen/hint_and_input_text"/>
        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">
        <TextView

        android:layout_width="@dimen/InputTextViewTextWidth"
        android:textColor="@color/text_color"
        android:background="@android:color/transparent"
        android:maxLines="1"

        android:text="Elevation above Sea Level"
        android:textSize="@dimen/hint_and_input_text"
        android:layout_height="@dimen/TileHeight"
        />
    <EditText
                            android:id="@+id/ElevationAboveSeaLevelFragmentEdiText"
        android:background="@drawable/shape"
                            android:hint="Elevation above Sea Level"
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:maxLines="1"
        android:imeOptions="actionNext"
        android:inputType="numberSigned"
                            android:textColor="@color/text_color"
                            android:layout_height="@dimen/TileHeight"
                            android:textSize="@dimen/hint_and_input_text"/>
                        <TextView
                            android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                            android:layout_height="@dimen/TileHeight"
                            android:textColor="@color/text_color"

                            android:text="ft"
                            android:id="@+id/UnitsElevationAboveSeaLevelFragmentTextView"
                            android:textSize="@dimen/hint_and_input_text"/>



        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">
        <TextView

                android:layout_width="@dimen/InputTextViewTextWidth"
                android:textColor="@color/text_color"
                android:background="@android:color/transparent"
                android:maxLines="1"

                android:text="Wet Bulb Temperature"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_height="@dimen/TileHeight"
                />
                        <EditText
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:id="@+id/wetBulbTemperatureGasFragmentEditText"
                            android:textColor="@color/text_color"

                            android:imeOptions="actionNext"
                            android:inputType="numberSigned"
                            android:background="@drawable/shape"
                            android:hint="Wet Bulb Temperature"
                            android:maxLines="1"

                            android:layout_height="@dimen/TileHeight"
                            android:textSize="@dimen/hint_and_input_text"/>
                        <TextView
                            android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                            android:layout_height="@dimen/TileHeight"

                            android:textColor="@color/text_color"
                            android:text="°C"
                            android:id="@+id/UnitsWetBulbtemperatureGasDensityFragmentTextView"

                            android:textSize="@dimen/hint_and_input_text"/>



        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>
        <TableRow
            android:layout_marginTop="@dimen/text_top_and_bottom_padding"
            android:layout_marginBottom="@dimen/text_top_and_bottom_padding">

        <TextView

                android:layout_width="@dimen/InputTextViewTextWidth"
                android:textColor="@color/text_color"
                android:background="@android:color/transparent"
                android:maxLines="1"

                android:text="Pressure at Sea Level"
                android:textSize="@dimen/hint_and_input_text"
                android:layout_height="@dimen/TileHeight"
                />
                        <EditText
                            android:layout_width="@dimen/InputEditTextWidth"
                            android:textColor="@color/text_color"
                            android:background="@drawable/shape"
                            android:hint="Pressure at Sea Level"
                            android:maxLines="1"

                            android:imeOptions="actionDone"
                            android:inputType="numberSigned"
                            android:textSize="@dimen/hint_and_input_text"
                            android:layout_height="@dimen/TileHeight"
                            android:id="@+id/seaLevelPressureGasFragmentEditText"/>
                        <TextView
                            android:layout_width="@dimen/InputUnitsTextViewTextWidth"
                            android:layout_height="@dimen/TileHeight"
                            android:textColor="@color/text_color"
                            android:text="kPa"
                            android:id="@+id/UnitsseaLevelPressureGasDensityFragmentTextView"

                            android:textSize="@dimen/hint_and_input_text"/>
        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>





    </TableLayout>
</ScrollView>
    </LinearLayout>